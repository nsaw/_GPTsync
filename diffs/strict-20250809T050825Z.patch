+  # --- LOCAL/FAST HOOKS ---
   - repo: local
     hooks:
+      - id: remove-trailing-whitespace
+        name: "Remove trailing whitespace"
+        entry: bash -c "for f in \"$@\"; do sed -i '' -e 's/[[:space:]]\+$//' \"$f\"; done"
